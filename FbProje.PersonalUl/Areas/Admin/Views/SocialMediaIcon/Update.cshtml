@model SocialMediaIconUpdateDto
@{
    ViewData["Title"] = "Add";
    Layout = "~/Areas/Admin/Views/Shared/View.cshtml";
}
<form asp-action="Update" asp-controller="SocialMediaIcon" method="post">
    <input type="hidden" asp-for="@Model.Id" />
     <div class="form-group">
         <label>Icon bilgisi:</label><span asp-validation-for="@Model.Icon"></span>
         <input  type="text" asp-for="@Model.Icon" class="form-control"/>
     </div>
      <div class="form-group">
         <label>Link:</label><span asp-validation-for="@Model.Link"></span>
         <input  type="text" asp-for="@Model.Link" class="form-control"/>
     </div>
     <div class="form-group">
         <button type="submit" class="btn btn-primary">Kaydet</button>
     </div>
</form>